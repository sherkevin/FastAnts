USER             if (index > 0) {
USER                 slides[index - 1].classList.add('prev');
USER             }
USER             
USER             currentSlide = index;
USER         }
USER 
USER         function nextSlide() {
USER             const nextIndex = (currentSlide + 1) % totalSlides;
USER             showSlide(nextIndex);
USER         }
USER 
USER         function prevSlide() {
USER             const prevIndex = (currentSlide - 1 + totalSlides) % totalSlides;
USER             showSlide(prevIndex);
USER         }
USER 
