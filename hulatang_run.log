ASSISTANT     <script>
ASSISTANT         let currentSlide = 0;
ASSISTANT         const slides = document.querySelectorAll('.slide');
ASSISTANT         const dots = document.querySelectorAll('.nav-dot');
ASSISTANT         const totalSlides = slides.length;
ASSISTANT 
ASSISTANT         function showSlide(index) {
ASSISTANT             // Hide all slides
ASSISTANT             slides.forEach(slide => {
ASSISTANT                 slide.classList.remove('active', 'prev');
ASSISTANT             });
ASSISTANT             
ASSISTANT             // Remove active class from all dots
ASSISTANT             dots.forEach(dot => {
ASSISTANT                 dot.classList.remove('active');
ASSISTANT             });
ASSISTANT             
ASSISTANT             // Show current slide
ASSISTANT             slides[index].classList.add('active');
ASSISTANT             dots[index].classList.add('active');
ASSISTANT             
ASSISTANT             // Add prev class to previous slide for transition
ASSISTANT             if (index > 0) {
ASSISTANT                 slides[index - 1].classList.add('prev');
ASSISTANT             }
ASSISTANT             
ASSISTANT             currentSlide = index;
ASSISTANT         }
ASSISTANT 
ASSISTANT         function nextSlide() {
ASSISTANT             const nextIndex = (currentSlide + 1) % totalSlides;
ASSISTANT             showSlide(nextIndex);
ASSISTANT         }
ASSISTANT 
ASSISTANT         function prevSlide() {
ASSISTANT             const prevIndex = (currentSlide - 1 + totalSlides) % totalSlides;
ASSISTANT             showSlide(prevIndex);
ASSISTANT         }
ASSISTANT 
ASSISTANT         // Event listeners
ASSISTANT         document.getElementById('nextArrow').addEventListener('click', nextSlide);
ASSISTANT         document.getElementById('prevArrow').addEventListener('click', prevSlide);
ASSISTANT 
ASSISTANT         // Dot navigation
ASSISTANT         dots.forEach((dot, index) => {
ASSISTANT             dot.addEventListener('click', () => {
ASSISTANT                 showSlide(index);
ASSISTANT             });
ASSISTANT         });
ASSISTANT 
ASSISTANT         // Keyboard navigation
ASSISTANT         document.addEventListener('keydown', (e) => {
ASSISTANT             if (e.key === 'ArrowRight' || e.key === ' ') {
