USER ```
USER #!/usr/bin/perl
USER 
USER use strict;
USER use warnings;
USER use IPC::Open2;
USER 
USER # An example hook script to integrate Watchman
USER # (https://facebook.github.io/watchman/) with git to speed up detecting
USER # new and modified files.
USER #
USER # The hook is passed a version (currently 2) and last update token
USER # formatted as a string and outputs to stdout a new update token and
USER # all files that have been modified since the update token. Paths must
USER # be relative to the root of the working tree and separated by a single NUL.
USER #
USER # To enable this hook, rename this file to "query-watchman" and set
USER # 'git config core.fsmonitor .git/hooks/query-watchman'
USER #
USER my ($version, $last_update_token) = @ARGV;
USER 
USER # Uncomment for debugging
USER # print STDERR "$0 $version $last_update_token\n";
USER 
USER # Check the hook interface version
USER if ($version ne 2) {
USER 	die "Unsupported query-fsmonitor hook version '$version'.\n" .
USER 	    "Falling back to scanning...\n";
USER }
USER 
USER my $git_work_tree = get_working_dir();
USER 
USER my $retry = 1;
USER 
USER my $json_pkg;
USER eval {
USER 	require JSON::XS;
USER 	$json_pkg = "JSON::XS";
USER 	1;
USER } or do {
USER 	require JSON::PP;
USER 	$json_pkg = "JSON::PP";
USER };
USER 
USER launch_watchman();
USER 
USER sub launch_watchman {
USER 	my $o = watchman_query();
USER 	if (is_work_tree_watched($o)) {
USER 		output_result($o->{clock}, @{$o->{files}});
USER 	}
USER }
USER 
USER sub output_result {
USER 	my ($clockid, @files) = @_;
USER 
USER 	# Uncomment for debugging watchman output
USER 	# open (my $fh, ">", ".git/watchman-output.out");
USER 	# binmode $fh, ":utf8";
USER 	# print $fh "$clockid\n@files\n";
USER 	# close $fh;
USER 
USER 	binmode STDOUT, ":utf8";
USER 	print $clockid;
USER 	print "\0";
USER 	local $, = "\0";
USER 	print @files;
USER }
USER 
USER sub watchman_clock {
USER 	my $response = qx/watchman clock "$git_work_tree"/;
USER 	die "Failed to get clock id on '$git_work_tree'.\n" .
USER 		"Falling back to scanning...\n" if $? != 0;
USER 
USER 	return $json_pkg->new->utf8->decode($response);
USER }
USER 
USER sub watchman_query {
USER 	my $pid = open2(\*CHLD_OUT, \*CHLD_IN, 'watchman -j --no-pretty')
USER 	or die "open2() failed: $!\n" .
USER 	"Falling back to scanning...\n";
USER 
USER 	# In the query expression below we're asking for names of files that
USER 	# changed since $last_update_token but not from the .git folder.
USER 	#
USER 	# To accomplish this, we're using the "since" generator to use the
USER 	# recency index to select candidate nodes and "fields" to limit the
USER 	# output to file names only. Then we're using the "expression" term to
USER 	# further constrain the results.
USER 	if (substr($last_update_token, 0, 1) eq "c") {
USER 		$last_update_token = "\"$last_update_token\"";
USER 	}
USER 	my $query = <<"	END";
USER 		["query", "$git_work_tree", {
USER 			"since": $last_update_token,
USER 			"fields": ["name"],
USER 			"expression": ["not", ["dirname", ".git"]]
USER 		}]
USER 	END
USER 
USER 	# Uncomment for debugging the watchman query
USER 	# open (my $fh, ">", ".git/watchman-query.json");
USER 	# print $fh $query;
USER 	# close $fh;
USER 
USER 	print CHLD_IN $query;
USER 	close CHLD_IN;
USER 	my $response = do {local $/; <CHLD_OUT>};
USER 
USER 	# Uncomment for debugging the watch response
USER 	# open ($fh, ">", ".git/watchman-response.json");
USER 	# print $fh $response;
USER 	# close $fh;
USER 
USER 	die "Watchman: command returned no output.\n" .
USER 	"Falling back to scanning...\n" if $response eq "";
USER 	die "Watchman: command returned invalid output: $response\n" .
USER 	"Falling back to scanning...\n" unless $response =~ /^\{/;
USER 
USER 	return $json_pkg->new->utf8->decode($response);
USER }
USER 
USER sub is_work_tree_watched {
USER 	my ($output) = @_;
USER 	my $error = $output->{error};
USER 	if ($retry > 0 and $error and $error =~ m/unable to resolve root .* directory (.*) is not watched/) {
USER 		$retry--;
USER 		my $response = qx/watchman watch "$git_work_tree"/;
USER 		die "Failed to make watchman watch '$git_work_tree'.\n" .
USER 		    "Falling back to scanning...\n" if $? != 0;
USER 		$output = $json_pkg->new->utf8->decode($response);
USER 		$error = $output->{error};
USER 		die "Watchman: $error.\n" .
USER 		"Falling back to scanning...\n" if $error;
USER 
USER 		# Uncomment for debugging watchman output
USER 		# open (my $fh, ">", ".git/watchman-output.out");
USER 		# close $fh;
USER 
USER 		# Watchman will always return all files on the first query so
USER 		# return the fast "everything is dirty" flag to git and do the
USER 		# Watchman query just to get it over with now so we won't pay
USER 		# the cost in git to look up each individual file.
USER 		my $o = watchman_clock();
USER 		$error = $output->{error};
USER 
USER 		die "Watchman: $error.\n" .
USER 		"Falling back to scanning...\n" if $error;
USER 
USER 		output_result($o->{clock}, ("/"));
USER 		$last_update_token = $o->{clock};
USER 
USER 		eval { launch_watchman() };
USER 		return 0;
USER 	}
USER 
USER 	die "Watchman: $error.\n" .
USER 	"Falling back to scanning...\n" if $error;
USER 
USER 	return 1;
USER }
USER 
USER sub get_working_dir {
USER 	my $working_dir;
USER 	if ($^O =~ 'msys' || $^O =~ 'cygwin') {
USER 		$working_dir = Win32::GetCwd();
USER 		$working_dir =~ tr/\\/\//;
USER 	} else {
USER 		require Cwd;
USER 		$working_dir = Cwd::cwd();
USER 	}
USER 
USER 	return $working_dir;
USER }
USER ```
