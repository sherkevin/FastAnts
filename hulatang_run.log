USER         let currentSlide = 0;
USER         const slides = document.querySelectorAll('.slide');
USER         const dots = document.querySelectorAll('.nav-dot');
USER         const totalSlides = slides.length;
USER 
USER         function showSlide(index) {
USER             // Hide all slides
USER             slides.forEach(slide => {
USER                 slide.classList.remove('active', 'prev');
USER             });
USER             
USER             // Remove active class from all dots
USER             dots.forEach(dot => {
